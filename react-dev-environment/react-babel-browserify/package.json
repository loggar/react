{
  "name": "react-bable-browserify",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "lite-server",
	"build": "browserify -t [ babelify --presets [ react ] ] src/index.js -o build/app.js",
	"watch": "watchify -t [ babelify --presets [ react ] ] src/index.js -o build/app.js -v"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "lite-server": "^2.3.0",
    "watchify": "^3.9.0"
  }
}
